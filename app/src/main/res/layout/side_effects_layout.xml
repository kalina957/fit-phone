<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/SideEffectsLayout"
        android:layout_width="408dp"
        android:layout_height="400dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chart"
        tools:layout_conversion_absoluteHeight="730dp"
        tools:layout_conversion_absoluteWidth="411dp">

        <ImageView
            android:id="@+id/imageSE3"
            android:layout_width="170dp"
            android:layout_height="140dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="34dp"
            android:adjustViewBounds="true"
            android:scaleType="fitXY"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageSE1"
            app:srcCompat="@mipmap/ic_launcher_round"
            tools:layout_conversion_absoluteHeight="72dp"
            tools:layout_conversion_absoluteWidth="72dp"
            tools:srcCompat="@tools:sample/avatars[6]" />

        <ImageView
            android:id="@+id/imageSE4"
            android:layout_width="170dp"
            android:layout_height="140dp"
            android:layout_marginTop="34dp"
            android:layout_marginEnd="24dp"
            android:adjustViewBounds="true"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageSE2"
            app:srcCompat="@mipmap/ic_launcher_round"
            tools:layout_conversion_absoluteHeight="72dp"
            tools:layout_conversion_absoluteWidth="72dp"
            tools:srcCompat="@tools:sample/avatars[6]" />

        <ImageView
            android:id="@+id/imageSE2"
            android:layout_width="170dp"
            android:layout_height="140dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="24dp"
            android:adjustViewBounds="true"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            app:srcCompat="@mipmap/ic_launcher_round"
            tools:layout_conversion_absoluteHeight="72dp"
            tools:layout_conversion_absoluteWidth="72dp"
            tools:srcCompat="@tools:sample/avatars[6]" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="SIDE EFFECTS"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageSE1"
            android:layout_width="170dp"
            android:layout_height="140dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="20dp"
            android:adjustViewBounds="true"
            android:scaleType="fitXY"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            app:srcCompat="@mipmap/ic_launcher_round"
            tools:layout_conversion_absoluteHeight="72dp"
            tools:layout_conversion_absoluteWidth="72dp"
            tools:srcCompat="@tools:sample/avatars[6]" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>